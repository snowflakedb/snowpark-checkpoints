{
  "name": "helloworld-sample",
  "displayName": "helloworld-sample",
  "description": "HelloWorld example for VS Code",
  "version": "0.0.1",
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./ --rootDir ./test --outDir ./out",
    "lint": "eslint",
    "envs:test": "NODE_ENV=$(grep NODE_ENV .env.test | cut -d '=' -f2)",
    "pretest": "npm run compile && npm run envs:test",
    "test": "node ./out/runTest.js"
  },
  "devDependencies": {
    "@eslint/js": "^9.13.0",
    "@stylistic/eslint-plugin": "^2.9.0",
    "@types/glob": "^7.2.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "^20",
    "@types/vscode": "^1.73.0",
    "@vscode/test-electron": "^2.3.9",
    "dotenv": "^16.4.7",
    "eslint": "^9.13.0",
    "glob": "^7.2.0",
    "minimatch": "^5.1.0",
    "mocha": "^10.2.0",
    "source-map-support": "^0.5.12",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.16.0"
  }
}
